:4000 {
    reverse_proxy {
        lb_policy least_conn
        health_uri /health/liveliness
        health_interval 20s
        health_timeout 5s
        transport http {
            read_timeout 5m
            write_timeout 5m
            dial_timeout 10s
        }
        to litellm-1:4000 litellm-2:4000 litellm-3:4000
    }
}
